<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>PRSTUDIOS | Sound Design & Music Composition</title>
  <link href="https://fonts.googleapis.com/css2?family=Inter:wght@400;700;900&family=JetBrains+Mono:wght@400&display=swap" rel="stylesheet">
  <style>
    * { margin: 0; padding: 0; box-sizing: border-box; cursor: none; }
    :root {
      --black: #0a0a0a;
      --white: #e8e4dc;
      --gray: #666;
      --mx: 50vw;
      --my: 50vh;
    }
    html { scroll-behavior: smooth; }
    body {
      background: var(--black);
      color: var(--white);
      font-family: 'Inter', sans-serif;
      overflow-x: hidden;
    }
    
    /* ========================================
       DESTRUCTION CANVAS
       ======================================== */
    #void {
      position: fixed;
      inset: 0;
      z-index: 99998;
      pointer-events: none;
    }
    
    /* Hidden sampler for pixel extraction */
    #sampler {
      position: fixed;
      top: 0; left: 0;
      pointer-events: none;
      z-index: -1;
      opacity: 0;
    }
    
    /* ========================================
       CHROMATIC ABERRATION LAYER
       ======================================== */
    .chroma {
      position: fixed;
      inset: 0;
      pointer-events: none;
      z-index: 99997;
      opacity: 0;
      transition: opacity 0.2s ease;
    }
    .chroma.active { opacity: 1; }
    .chroma::before,
    .chroma::after {
      content: '';
      position: absolute;
      inset: 0;
      mix-blend-mode: screen;
    }
    .chroma::before {
      background: radial-gradient(
        circle 150px at var(--mx) var(--my),
        rgba(255, 0, 60, 0.12) 0%,
        rgba(255, 0, 60, 0.04) 40%,
        transparent 70%
      );
      transform: translate(-4px, 0);
    }
    .chroma::after {
      background: radial-gradient(
        circle 150px at var(--mx) var(--my),
        rgba(0, 150, 255, 0.12) 0%,
        rgba(0, 150, 255, 0.04) 40%,
        transparent 70%
      );
      transform: translate(4px, 0);
    }
    
    /* ========================================
       SCAN LINES - CRT effect
       ======================================== */
    .scanlines {
      position: fixed;
      inset: 0;
      pointer-events: none;
      z-index: 100001;
      background: repeating-linear-gradient(
        0deg,
        transparent 0px,
        transparent 2px,
        rgba(0, 0, 0, 0.12) 2px,
        rgba(0, 0, 0, 0.12) 4px
      );
      opacity: 0.5;
    }
    
    /* ========================================
       WARPABLE ELEMENTS
       ======================================== */
    .warp {
      transition: transform 0.12s cubic-bezier(0.25, 0.46, 0.45, 0.94),
                  filter 0.12s ease;
      will-change: transform, filter;
      position: relative;
    }
    
    /* Glitch effect */
    .warp.glitch {
      animation: glitchShake 0.15s infinite;
    }
    .warp.glitch::before,
    .warp.glitch::after {
      content: attr(data-text);
      position: absolute;
      top: 0;
      left: 0;
      width: 100%;
      height: 100%;
      opacity: 0.8;
      pointer-events: none;
    }
    .warp.glitch::before {
      color: #ff0050;
      animation: glitchR 0.2s infinite;
      clip-path: polygon(0 0, 100% 0, 100% 45%, 0 45%);
    }
    .warp.glitch::after {
      color: #00d4ff;
      animation: glitchB 0.2s infinite;
      clip-path: polygon(0 55%, 100% 55%, 100% 100%, 0 100%);
    }
    @keyframes glitchShake {
      0%, 100% { transform: translate(0) var(--warp-transform); }
      20% { transform: translate(-2px, 1px) var(--warp-transform); }
      40% { transform: translate(2px, -1px) var(--warp-transform); }
      60% { transform: translate(-1px, -2px) var(--warp-transform); }
      80% { transform: translate(1px, 2px) var(--warp-transform); }
    }
    @keyframes glitchR {
      0%, 100% { transform: translate(0); }
      25% { transform: translate(-3px, 0); }
      75% { transform: translate(3px, 0); }
    }
    @keyframes glitchB {
      0%, 100% { transform: translate(0); }
      25% { transform: translate(3px, 0); }
      75% { transform: translate(-3px, 0); }
    }
    
    @media (max-width: 768px) {
      #void, #sampler, .chroma, .scanlines { display: none; }
      * { cursor: auto; }
      .warp { transform: none !important; filter: none !important; }
    }
    
    /* ========================================
       FILM GRAIN
       ======================================== */
    .grain {
      position: fixed;
      inset: -50%;
      width: 200%;
      height: 200%;
      pointer-events: none;
      z-index: 100002;
      opacity: 0.22;
      background-image: url("data:image/svg+xml,%3Csvg viewBox='0 0 256 256' xmlns='http://www.w3.org/2000/svg'%3E%3Cfilter id='n'%3E%3CfeTurbulence type='fractalNoise' baseFrequency='0.85' numOctaves='3'/%3E%3CfeColorMatrix type='saturate' values='0'/%3E%3C/filter%3E%3Crect width='100%25' height='100%25' filter='url(%23n)'/%3E%3C/svg%3E");
      animation: grain 0.08s steps(4) infinite;
      mix-blend-mode: overlay;
    }
    @keyframes grain {
      0% { transform: translate(0); }
      25% { transform: translate(-1%, 1%); }
      50% { transform: translate(1%, -1%); }
      75% { transform: translate(-1%, -1%); }
    }
    
    .vignette {
      position: fixed;
      inset: 0;
      pointer-events: none;
      z-index: 2;
      background: radial-gradient(ellipse 70% 70% at 50% 50%, transparent 0%, rgba(0,0,0,0.65) 100%);
    }
    
    /* Waveform */
    .waveform {
      position: fixed;
      bottom: 48px;
      right: 48px;
      display: flex;
      align-items: center;
      gap: 3px;
      z-index: 20;
      opacity: 0.2;
    }
    .waveform span {
      width: 2px;
      background: var(--white);
      animation: wave 1.2s ease-in-out infinite;
    }
    .waveform span:nth-child(1) { height: 8px; }
    .waveform span:nth-child(2) { height: 16px; animation-delay: 0.1s; }
    .waveform span:nth-child(3) { height: 12px; animation-delay: 0.2s; }
    .waveform span:nth-child(4) { height: 20px; animation-delay: 0.3s; }
    .waveform span:nth-child(5) { height: 8px; animation-delay: 0.4s; }
    .waveform span:nth-child(6) { height: 14px; animation-delay: 0.5s; }
    .waveform span:nth-child(7) { height: 10px; animation-delay: 0.6s; }
    @keyframes wave {
      0%, 100% { transform: scaleY(1); }
      50% { transform: scaleY(0.4); }
    }
    @media (max-width: 768px) { .waveform { display: none; } }
    
    /* Navigation */
    nav {
      position: fixed;
      top: 0;
      left: 0;
      right: 0;
      z-index: 50;
      padding: 32px 48px;
    }
    .nav-inner {
      max-width: 1400px;
      margin: 0 auto;
      display: flex;
      justify-content: space-between;
      align-items: center;
    }
    .logo {
      font-weight: 900;
      font-size: 16px;
      letter-spacing: 0.2em;
      opacity: 0;
      animation: fadeIn 0.6s ease forwards 0.2s;
    }
    .contact-btn {
      font-family: 'JetBrains Mono', monospace;
      font-size: 10px;
      letter-spacing: 0.15em;
      color: var(--white);
      text-decoration: none;
      padding: 12px 24px;
      border: 1px solid rgba(232,228,220,0.3);
      transition: all 0.3s ease;
      opacity: 0;
      animation: fadeIn 0.6s ease forwards 0.4s;
    }
    .contact-btn:hover {
      background: var(--white);
      color: var(--black);
    }
    
    /* Hero */
    .hero {
      min-height: 100vh;
      display: flex;
      align-items: center;
      justify-content: center;
      padding: 120px 32px 160px;
      position: relative;
      z-index: 10;
    }
    .hero-content {
      text-align: center;
      max-width: 100%;
      margin-bottom: 60px;
    }
    
    .top-tag {
      font-family: 'JetBrains Mono', monospace;
      font-size: clamp(9px, 1.2vw, 11px);
      letter-spacing: 0.4em;
      color: var(--gray);
      margin-bottom: 48px;
      line-height: 2.4;
      opacity: 0;
      animation: slideUp 0.8s ease forwards 0.3s;
    }
    
    h1 {
      font-weight: 900;
      font-size: clamp(5rem, 25vw, 22rem);
      line-height: 0.82;
      letter-spacing: -0.05em;
      text-transform: uppercase;
    }
    .line { 
      display: block;
      opacity: 0;
      transform: translateY(60px);
      animation: slideUp 0.8s ease forwards;
      position: relative;
    }
    .line:nth-child(1) { animation-delay: 0.4s; }
    .line:nth-child(2) { animation-delay: 0.5s; }
    .line:nth-child(3) { animation-delay: 0.6s; }
    .line.outline {
      color: transparent;
      -webkit-text-stroke: 1.5px var(--white);
    }
    .line.small {
      font-size: clamp(1.5rem, 6vw, 5rem);
      letter-spacing: 0.05em;
      margin: 16px 0;
    }
    .line.small { animation: slideUpFade 0.8s ease forwards 0.5s; }
    
    @keyframes slideUp { to { opacity: 1; transform: translateY(0); } }
    @keyframes slideUpFade { to { opacity: 0.4; transform: translateY(0); } }
    @keyframes fadeIn { to { opacity: 1; } }
    
    .subtitle {
      margin-top: 64px;
      font-family: 'JetBrains Mono', monospace;
      font-size: 11px;
      letter-spacing: 0.4em;
      color: var(--white);
      opacity: 0;
      animation: fadeIn 0.8s ease forwards 0.8s;
    }
    
    .services {
      margin-top: 16px;
      font-family: 'JetBrains Mono', monospace;
      font-size: 10px;
      letter-spacing: 0.3em;
      color: var(--gray);
      opacity: 0;
      animation: fadeIn 0.8s ease forwards 0.9s;
    }
    
    .scroll-hint {
      position: absolute;
      bottom: 32px;
      left: 50%;
      transform: translateX(-50%);
      font-family: 'JetBrains Mono', monospace;
      font-size: 10px;
      letter-spacing: 0.3em;
      color: var(--gray);
      display: flex;
      flex-direction: column;
      align-items: center;
      gap: 16px;
      text-decoration: none;
      opacity: 0;
      animation: fadeIn 0.8s ease forwards 1.2s;
    }
    .scroll-line {
      width: 1px;
      height: 48px;
      background: linear-gradient(to bottom, var(--gray), transparent);
    }
    
    /* Work Section */
    .work {
      padding: 120px 48px;
      position: relative;
      z-index: 10;
    }
    .work-inner { max-width: 1000px; margin: 0 auto; }
    
    .section-header {
      margin-bottom: 64px;
      padding-bottom: 24px;
      border-bottom: 1px solid rgba(232,228,220,0.1);
      display: flex;
      justify-content: space-between;
      align-items: baseline;
    }
    .section-title {
      font-weight: 900;
      font-size: clamp(48px, 12vw, 100px);
      letter-spacing: -0.03em;
      position: relative;
    }
    .section-count {
      font-family: 'JetBrains Mono', monospace;
      font-size: 12px;
      color: var(--gray);
    }
    
    .project {
      display: block;
      padding: 48px 0;
      border-bottom: 1px solid rgba(232,228,220,0.05);
      text-decoration: none;
      color: var(--white);
      transition: padding 0.4s ease;
      position: relative;
    }
    .project:hover { padding-left: 32px; }
    .project-title {
      font-weight: 900;
      font-size: clamp(28px, 8vw, 72px);
      letter-spacing: -0.03em;
      text-transform: uppercase;
      line-height: 1;
      transition: all 0.4s;
      position: relative;
    }
    .project:hover .project-title {
      color: transparent;
      -webkit-text-stroke: 1px var(--white);
    }
    .project-meta {
      display: flex;
      gap: 32px;
      margin-top: 12px;
      font-family: 'JetBrains Mono', monospace;
      font-size: 10px;
      letter-spacing: 0.2em;
      color: var(--gray);
    }
    .project-arrow {
      position: absolute;
      right: 0;
      top: 50%;
      transform: translateY(-50%) translateX(10px);
      opacity: 0;
      font-size: 20px;
      transition: all 0.4s ease;
    }
    .project:hover .project-arrow {
      opacity: 1;
      transform: translateY(-50%) translateX(0);
    }
    
    .divider {
      padding: 32px 0;
      text-align: center;
      font-family: 'JetBrains Mono', monospace;
      font-size: 10px;
      letter-spacing: 0.5em;
      color: rgba(232,228,220,0.1);
    }
    
    /* Contact */
    .contact {
      padding: 120px 48px;
      text-align: center;
      position: relative;
      z-index: 10;
    }
    .contact-inner { max-width: 900px; margin: 0 auto; }
    .contact-label {
      font-family: 'JetBrains Mono', monospace;
      font-size: 10px;
      letter-spacing: 0.5em;
      color: var(--gray);
      margin-bottom: 32px;
    }
    .contact-title {
      font-weight: 900;
      font-size: clamp(48px, 15vw, 140px);
      letter-spacing: -0.04em;
      line-height: 0.85;
      text-transform: uppercase;
      position: relative;
    }
    .contact-title .outline {
      color: transparent;
      -webkit-text-stroke: 1.5px var(--white);
    }
    .contact-email {
      display: inline-block;
      margin-top: 64px;
      font-family: 'JetBrains Mono', monospace;
      font-size: clamp(12px, 1.5vw, 16px);
      letter-spacing: 0.1em;
      color: var(--gray);
      text-decoration: none;
      padding-bottom: 8px;
      border-bottom: 1px solid var(--gray);
      transition: all 0.3s;
    }
    .contact-email:hover {
      color: var(--white);
      border-color: var(--white);
    }
    
    .social {
      display: flex;
      justify-content: center;
      gap: 48px;
      margin-top: 64px;
    }
    .social a {
      font-family: 'JetBrains Mono', monospace;
      font-size: 10px;
      letter-spacing: 0.2em;
      color: var(--gray);
      text-decoration: none;
      transition: color 0.3s;
    }
    .social a:hover { color: var(--white); }
    
    footer {
      padding: 48px;
      display: flex;
      justify-content: space-between;
      font-family: 'JetBrains Mono', monospace;
      font-size: 9px;
      letter-spacing: 0.2em;
      color: rgba(232,228,220,0.2);
    }
    
    @media (max-width: 768px) {
      nav { padding: 24px; }
      .hero { padding: 100px 24px 160px; }
      .hero-content { margin-bottom: 80px; }
      .work { padding: 80px 24px; }
      .contact { padding: 80px 24px; }
      footer { padding: 32px 24px; flex-direction: column; gap: 8px; }
      .project-arrow { display: none; }
    }
  </style>
</head>
<body>
  
  <canvas id="void"></canvas>
  <canvas id="sampler"></canvas>
  <div class="chroma"></div>
  <div class="scanlines"></div>
  <div class="grain"></div>
  <div class="vignette"></div>
  
  <div class="waveform">
    <span></span><span></span><span></span><span></span><span></span><span></span><span></span>
  </div>
  
  <nav>
