  <script src="https://html2canvas.hertzen.com/dist/html2canvas.min.js"></script>
  <script>
    // ================================================
    // ULTIMATE CINEMATIC DESTRUCTION ENGINE
    // Everything combined into one massive effect
    // ================================================
    
    const canvas = document.getElementById('void');
    const ctx = canvas.getContext('2d');
    const samplerCanvas = document.getElementById('sampler');
    const samplerCtx = samplerCanvas.getContext('2d', { willReadFrequently: true });
    const chroma = document.querySelector('.chroma');
    const root = document.documentElement;
    const warpElements = document.querySelectorAll('.warp');
    
    let W, H;
    let mx = 0, my = 0;
    let cx = 0, cy = 0;
    let speed = 0;
    let time = 0;
    let isHovering = false;
    let lastMx = 0, lastMy = 0;
    let pageSnapshot = null;
    let lastSnapshotTime = 0;
    
    // ============================================
    // PARTICLE SYSTEMS
    // ============================================
    const pixels = [];          // Torn pixel debris from actual page
    const letters = [];         // Torn text characters
    const accretion = [];       // Accretion disk particles
    const tearLines = [];       // Reality tear lines
    const energyRings = [];     // Radiating energy rings
    
    const MAX_PIXELS = 250;
    const MAX_LETTERS = 100;
    const MAX_ACCRETION = 150;
    
    const WARP_RADIUS = 350;
    const GLITCH_RADIUS = 180;
    const TEAR_RADIUS = 220;
    
    // ============================================
    // SETUP
    // ============================================
    function resize() {
      W = canvas.width = window.innerWidth;
      H = canvas.height = window.innerHeight;
      samplerCanvas.width = W;
      samplerCanvas.height = H;
      captureSnapshot();
    }
    resize();
    window.addEventListener('resize', resize);
    
    // Capture page for pixel sampling
    async function captureSnapshot() {
      const now = Date.now();
      if (now - lastSnapshotTime < 3000) return;
      lastSnapshotTime = now;
      
      try {
        const body = document.body;
        const snapshot = await html2canvas(body, {
          backgroundColor: '#0a0a0a',
          scale: 0.4,
          logging: false,
          useCORS: true,
          ignoreElements: el => el.id === 'void' || el.id === 'sampler' || el.classList.contains('chroma') || el.classList.contains('scanlines') || el.classList.contains('grain')
        });
        pageSnapshot = snapshot;
        samplerCtx.drawImage(snapshot, 0, 0, W, H);
      } catch(e) {
        createFallbackSnapshot();
      }
    }
    
    function createFallbackSnapshot() {
      samplerCtx.fillStyle = '#0a0a0a';
      samplerCtx.fillRect(0, 0, W, H);
      
      document.querySelectorAll('h1, h2, h3, p, a, span, .logo, .project-title').forEach(el => {
        const rect = el.getBoundingClientRect();
        const style = getComputedStyle(el);
        samplerCtx.fillStyle = style.color || '#e8e4dc';
        samplerCtx.globalAlpha = 0.9;
        samplerCtx.fillRect(rect.left, rect.top, rect.width, rect.height);
      });
      samplerCtx.globalAlpha = 1;
    }
    
    setTimeout(captureSnapshot, 600);
    
    // Mouse tracking
    document.addEventListener('mousemove', e => {
      lastMx = mx;
      lastMy = my;
      mx = e.clientX;
      my = e.clientY;
      speed = Math.hypot(mx - lastMx, my - lastMy);
      
      root.style.setProperty('--mx', mx + 'px');
      root.style.setProperty('--my', my + 'px');
    });
    
    // Hover detection
    document.querySelectorAll('a, button, .project').forEach(el => {
      el.addEventListener('mouseenter', () => isHovering = true);
      el.addEventListener('mouseleave', () => isHovering = false);
    });
    
    // ============================================
    // SAMPLE REAL PIXELS FROM PAGE
    // ============================================
    function samplePixelColor(x, y) {
      try {
        const data = samplerCtx.getImageData(
          Math.floor(x * 0.4),
          Math.floor(y * 0.4),
          1, 1
        ).data;
        return { r: data[0], g: data[1], b: data[2] };
      } catch(e) {
        return { r: 232, g: 228, b: 220 };
      }
    }
    
    // ============================================
    // ELEMENT WARPING - Pull content toward cursor
    // ============================================
    function warpDOMElements() {
      let anyGlitching = false;
      let anyWarping = false;
      
      warpElements.forEach(el => {
        const rect = el.getBoundingClientRect();
        const elX = rect.left + rect.width / 2;
        const elY = rect.top + rect.height / 2;
        const dx = mx - elX;
        const dy = my - elY;
        const dist = Math.hypot(dx, dy);
        
        if (dist < WARP_RADIUS) {
          anyWarping = true;
          const influence = Math.pow(1 - dist / WARP_RADIUS, 2.5);
          const angle = Math.atan2(dy, dx);
          
          // Magnetic pull toward cursor
          const pull = influence * 30;
          const pullX = Math.cos(angle) * pull;
          const pullY = Math.sin(angle) * pull;
          
          // Rotation — twist toward void
          const rotation = influence * (dx > 0 ? 5 : -5);
          
          // Skew — being sucked in
          const skewX = influence * 6 * Math.sign(dx);
          const skewY = influence * 2 * Math.sign(dy);
          
          // Scale — compress toward center
          const scaleX = 1 + influence * 0.08;
          const scaleY = 1 - influence * 0.05;
          
          el.style.transform = `
            translate(${pullX}px, ${pullY}px)
            rotate(${rotation}deg)
            skew(${skewX}deg, ${skewY}deg)
            scale(${scaleX}, ${scaleY})
          `;
          el.style.setProperty('--warp-transform', '');
          
          // Glitch at close range
          if (dist < GLITCH_RADIUS) {
            const blur = (1 - dist / GLITCH_RADIUS) * 2;
            el.style.filter = `blur(${blur}px)`;
            el.classList.add('glitch');
            anyGlitching = true;
          } else {
            el.style.filter = '';
            el.classList.remove('glitch');
          }
        } else {
          el.style.transform = '';
          el.style.filter = '';
          el.classList.remove('glitch');
        }
      });
      
      // Chromatic aberration when warping or moving fast
      if (anyGlitching || anyWarping || speed > 12) {
        chroma.classList.add('active');
      } else {
        chroma.classList.remove('active');
      }
    }
    
    // ============================================
    // TEAR REAL PIXELS FROM PAGE
    // ============================================
    function tearPixels() {
      if (pixels.length >= MAX_PIXELS) return;
      
      const tearIntensity = Math.min(speed / 8, 1) + 0.15;
      const numPixels = Math.floor(3 + speed * 0.4);
      
      for (let i = 0; i < numPixels; i++) {
        if (pixels.length >= MAX_PIXELS) break;
        if (Math.random() > tearIntensity) continue;
        
        const angle = Math.random() * Math.PI * 2;
        const dist = 25 + Math.random() * TEAR_RADIUS;
        const px = mx + Math.cos(angle) * dist;
        const py = my + Math.sin(angle) * dist;
        
        if (px < 0 || px > W || py < 0 || py > H) continue;
        
        // Sample REAL color from page
        const color = samplePixelColor(px, py);
        
        // Skip pure background
        if (color.r < 15 && color.g < 15 && color.b < 15) {
          if (Math.random() > 0.15) continue;
        }
        
        const size = 2 + Math.random() * 6;
        
        pixels.push({
          x: px, y: py,
          size,
          ...color,
          life: 1,
          decay: 0.007 + Math.random() * 0.008,
          orbitAngle: Math.random() * Math.PI * 2,
          orbitSpeed: 0.025 + Math.random() * 0.045,
          orbitRadius: 35 + Math.random() * 70,
          rotation: Math.random() * Math.PI * 2,
          rotationSpeed: (Math.random() - 0.5) * 0.35,
        });
      }
    }
    
    // ============================================
    // TEAR LETTERS FROM TEXT
    // ============================================
    function tearLetters() {
      if (letters.length >= MAX_LETTERS) return;
      
      warpElements.forEach(el => {
        const rect = el.getBoundingClientRect();
        const text = el.textContent;
        if (!text || text.length < 2) return;
        
        const closestX = Math.max(rect.left, Math.min(mx, rect.right));
        const closestY = Math.max(rect.top, Math.min(my, rect.bottom));
        const dist = Math.hypot(mx - closestX, my - closestY);
        
        const prob = Math.pow(1 - dist / TEAR_RADIUS, 2.5) * 0.35 * (1 + speed * 0.02);
        
        if (dist < TEAR_RADIUS && Math.random() < prob && letters.length < MAX_LETTERS) {
          const char = text[Math.floor(Math.random() * text.length)];
          if (!char.trim()) return;
          
          const style = getComputedStyle(el);
          const isOutline = el.classList.contains('outline');
          
          letters.push({
            char,
            x: closestX + (Math.random() - 0.5) * 40,
            y: closestY + (Math.random() - 0.5) * 40,
            size: Math.min(parseFloat(style.fontSize) * 0.45, 52),
            weight: style.fontWeight,
            isOutline,
            life: 1,
            decay: 0.005 + Math.random() * 0.006,
            orbitAngle: Math.random() * Math.PI * 2,
            orbitSpeed: 0.02 + Math.random() * 0.035,
            orbitRadius: 45 + Math.random() * 75,
          });
        }
      });
    }
    
    // ============================================
    // ACCRETION DISK
    // ============================================
    function spawnAccretion() {
      if (accretion.length >= MAX_ACCRETION || Math.random() > 0.55) return;
      
      accretion.push({
        angle: Math.random() * Math.PI * 2,
        radius: 18 + Math.random() * 55,
        speed: 0.018 + Math.random() * 0.032,
        size: 1 + Math.random() * 2.5,
        life: 1,
        decay: 0.003 + Math.random() * 0.004,
        brightness: 0.25 + Math.random() * 0.75,
        type: Math.random() > 0.6 ? 'hot' : 'cold', // Orange or blue
      });
    }
    
    // ============================================
    // REALITY TEAR LINES
    // ============================================
    function spawnTearLines() {
      if (speed < 12 || tearLines.length > 15) return;
      
      const num = Math.floor(speed / 8);
      for (let i = 0; i < num; i++) {
        const moveAngle = Math.atan2(my - lastMy, mx - lastMx);
        const angle = moveAngle + (Math.random() - 0.5) * 1.2;
        tearLines.push({
          x: cx,
          y: cy,
          angle,
          length: 40 + Math.random() * 70,
          life: 1,
          decay: 0.04 + Math.random() * 0.03,
        });
      }
    }
    
    // ============================================
    // ENERGY RINGS - Radiate outward
    // ============================================
    function spawnEnergyRing() {
      if (speed < 8 || energyRings.length > 5) return;
      if (Math.random() > 0.3) return;
      
      energyRings.push({
        x: cx,
        y: cy,
        radius: 20,
        maxRadius: 150 + speed * 3,
        life: 1,
        decay: 0.025,
      });
    }
    
    // ============================================
    // MAIN RENDER LOOP
    // ============================================
    function render() {
      time += 0.016;
      
      // Smooth cursor
      cx += (mx - cx) * 0.1;
      cy += (my - cy) * 0.1;
      
      ctx.clearRect(0, 0, W, H);
      
      const intensity = Math.min(speed / 12, 1);
      const hover = isHovering ? 1.3 : 1;
      
      // Warp DOM elements
      warpDOMElements();
      
      // ============================================
      // 1. GRAVITATIONAL FIELD GLOW
      // ============================================
      const fieldRadius = (280 + intensity * 120) * hover;
      const fieldGrad = ctx.createRadialGradient(cx, cy, 0, cx, cy, fieldRadius);
      fieldGrad.addColorStop(0, `rgba(255, 250, 240, ${0.1 + intensity * 0.1})`);
      fieldGrad.addColorStop(0.25, `rgba(232, 228, 220, ${0.05 + intensity * 0.06})`);
      fieldGrad.addColorStop(0.5, `rgba(200, 180, 160, ${0.02 + intensity * 0.03})`);
      fieldGrad.addColorStop(1, 'transparent');
      ctx.fillStyle = fieldGrad;
      ctx.beginPath();
      ctx.arc(cx, cy, fieldRadius, 0, Math.PI * 2);
      ctx.fill();
      
      // ============================================
      // 2. ENERGY RINGS - Radiating outward
      // ============================================
      spawnEnergyRing();
      
      for (let i = energyRings.length - 1; i >= 0; i--) {
        const ring = energyRings[i];
        ring.radius += (ring.maxRadius - ring.radius) * 0.08;
        ring.life -= ring.decay;
        
        if (ring.life > 0) {
          ctx.strokeStyle = `rgba(232, 228, 220, ${ring.life * 0.3})`;
          ctx.lineWidth = 1.5 * ring.life;
          ctx.beginPath();
          ctx.arc(ring.x, ring.y, ring.radius, 0, Math.PI * 2);
          ctx.stroke();
        } else {
          energyRings.splice(i, 1);
        }
      }
      
      // ============================================
      // 3. GRAVITATIONAL LENSING RINGS (Pulsing)
      // ============================================
      const numRings = 7;
      for (let i = 0; i < numRings; i++) {
        const baseRadius = (22 + i * 16) * hover;
        const pulse = Math.sin(time * 3 + i * 0.8) * 3;
        const alpha = (0.12 - i * 0.014) * (1 + intensity * 0.6);
        
        ctx.strokeStyle = `rgba(232, 228, 220, ${alpha})`;
        ctx.lineWidth = 1 + (numRings - i) * 0.1;
        ctx.beginPath();
        ctx.arc(cx, cy, baseRadius + pulse, 0, Math.PI * 2);
        ctx.stroke();
      }
      
      // ============================================
      // 4. REALITY TEAR LINES
      // ============================================
      spawnTearLines();
      
      for (let i = tearLines.length - 1; i >= 0; i--) {
        const line = tearLines[i];
        line.life -= line.decay;
        
        if (line.life > 0) {
          const len = line.length * line.life;
          
          // Chromatic split on lines
          ctx.strokeStyle = `rgba(255, 100, 100, ${line.life * 0.3})`;
          ctx.lineWidth = 1;
          ctx.beginPath();
          ctx.moveTo(line.x - 2, line.y);
          ctx.lineTo(line.x - 2 + Math.cos(line.angle) * len, line.y + Math.sin(line.angle) * len);
          ctx.stroke();
          
          ctx.strokeStyle = `rgba(100, 180, 255, ${line.life * 0.3})`;
          ctx.beginPath();
          ctx.moveTo(line.x + 2, line.y);
          ctx.lineTo(line.x + 2 + Math.cos(line.angle) * len, line.y + Math.sin(line.angle) * len);
          ctx.stroke();
          
          ctx.strokeStyle = `rgba(232, 228, 220, ${line.life * 0.7})`;
          ctx.lineWidth = 2 * line.life;
          ctx.beginPath();
          ctx.moveTo(line.x, line.y);
          ctx.lineTo(line.x + Math.cos(line.angle) * len, line.y + Math.sin(line.angle) * len);
          ctx.stroke();
        } else {
          tearLines.splice(i, 1);
        }
      }
      
      // ============================================
      // 5. ACCRETION DISK
      // ============================================
      spawnAccretion();
      
      for (let i = accretion.length - 1; i >= 0; i--) {
        const p = accretion[i];
        
        p.angle += p.speed * (1 + (1 - p.life) * 0.5);
        p.radius *= 0.995;
        p.life -= p.decay;
        
        const ax = cx + Math.cos(p.angle) * p.radius * hover;
        const ay = cy + Math.sin(p.angle) * p.radius * hover * 0.28;
        
        if (p.life > 0 && p.radius > 4) {
          const alpha = p.life * p.brightness;
          const glow = ctx.createRadialGradient(ax, ay, 0, ax, ay, p.size * 6);
          
          if (p.type === 'hot') {
            glow.addColorStop(0, `rgba(255, 220, 180, ${alpha})`);
            glow.addColorStop(0.4, `rgba(255, 160, 80, ${alpha * 0.6})`);
            glow.addColorStop(1, 'transparent');
          } else {
            glow.addColorStop(0, `rgba(180, 220, 255, ${alpha})`);
            glow.addColorStop(0.4, `rgba(80, 160, 255, ${alpha * 0.6})`);
            glow.addColorStop(1, 'transparent');
          }
          
          ctx.fillStyle = glow;
          ctx.beginPath();
          ctx.arc(ax, ay, p.size * 6, 0, Math.PI * 2);
          ctx.fill();
        } else {
          accretion.splice(i, 1);
        }
      }
      
      // ============================================
      // 6. EVENT HORIZON
      // ============================================
      const horizonRadius = (18 + intensity * 10) * hover;
      
      // Dark void
      const voidGrad = ctx.createRadialGradient(cx, cy, 0, cx, cy, horizonRadius * 3.5);
      voidGrad.addColorStop(0, 'rgba(0, 0, 0, 0.99)');
      voidGrad.addColorStop(0.25, 'rgba(0, 0, 0, 0.9)');
      voidGrad.addColorStop(0.5, 'rgba(0, 0, 0, 0.6)');
      voidGrad.addColorStop(0.75, 'rgba(0, 0, 0, 0.2)');
      voidGrad.addColorStop(1, 'transparent');
      ctx.fillStyle = voidGrad;
      ctx.beginPath();
      ctx.arc(cx, cy, horizonRadius * 3.5, 0, Math.PI * 2);
      ctx.fill();
      
      // Photon sphere - bright ring
      const photonPulse = 1 + Math.sin(time * 4) * 0.1;
      ctx.strokeStyle = `rgba(255, 252, 245, ${0.7 + intensity * 0.25})`;
      ctx.lineWidth = 2.5 * photonPulse;
      ctx.beginPath();
      ctx.arc(cx, cy, horizonRadius, 0, Math.PI * 2);
      ctx.stroke();
      
      // Inner glow
      const innerGlow = ctx.createRadialGradient(cx, cy, horizonRadius * 0.2, cx, cy, horizonRadius);
      innerGlow.addColorStop(0, 'rgba(255, 255, 255, 0.2)');
      innerGlow.addColorStop(0.5, 'rgba(255, 230, 200, 0.35)');
      innerGlow.addColorStop(1, 'rgba(255, 200, 150, 0.5)');
      ctx.fillStyle = innerGlow;
      ctx.beginPath();
      ctx.arc(cx, cy, horizonRadius, 0, Math.PI * 2);
      ctx.fill();
      
      // Singularity core
      const corePulse = 1 + Math.sin(time * 5) * 0.15;
      ctx.fillStyle = '#fff';
      ctx.shadowColor = '#fff';
      ctx.shadowBlur = 35 + intensity * 30;
      ctx.beginPath();
      ctx.arc(cx, cy, 4.5 * hover * corePulse, 0, Math.PI * 2);
      ctx.fill();
      ctx.shadowBlur = 0;
      
      // ============================================
      // 7. PIXEL DEBRIS - Real page pixels
      // ============================================
      tearPixels();
      
      for (let i = pixels.length - 1; i >= 0; i--) {
        const p = pixels[i];
        
        const dx = cx - p.x;
        const dy = cy - p.y;
        const dist = Math.hypot(dx, dy);
        
        const gravity = 2200 / (dist * dist + 90);
        
        p.orbitAngle += p.orbitSpeed * (1 + (1 - p.life) * 2.5);
        p.orbitRadius *= 0.965;
        
        const orbitX = Math.cos(p.orbitAngle) * p.orbitRadius * p.life;
        const orbitY = Math.sin(p.orbitAngle) * p.orbitRadius * p.life * 0.35;
        
        p.x += (dx / dist) * gravity + orbitX * 0.02;
        p.y += (dy / dist) * gravity + orbitY * 0.02;
        p.rotation += p.rotationSpeed;
        p.life -= p.decay;
        
        if (p.life > 0 && dist > 8) {
          const size = p.size * p.life;
          
          ctx.save();
          ctx.translate(p.x, p.y);
          ctx.rotate(p.rotation);
          
          // Chromatic split
          if (p.life < 0.55) {
            const split = (0.55 - p.life) * 8;
            ctx.fillStyle = `rgba(${Math.min(255, p.r + 100)}, ${p.g * 0.2}, ${p.b * 0.2}, ${p.life * 0.45})`;
            ctx.fillRect(-size/2 - split, -size/2, size, size);
            ctx.fillStyle = `rgba(${p.r * 0.2}, ${p.g * 0.2}, ${Math.min(255, p.b + 100)}, ${p.life * 0.45})`;
            ctx.fillRect(-size/2 + split, -size/2, size, size);
          }
          
          // Main pixel
          ctx.fillStyle = `rgba(${p.r}, ${p.g}, ${p.b}, ${p.life})`;
          ctx.fillRect(-size/2, -size/2, size, size);
          
          // Glow on bright pixels
          if (p.r > 120 || p.g > 120 || p.b > 120) {
            ctx.shadowColor = `rgb(${p.r}, ${p.g}, ${p.b})`;
            ctx.shadowBlur = 6 * p.life;
            ctx.fillRect(-size/2, -size/2, size, size);
            ctx.shadowBlur = 0;
          }
          
          ctx.restore();
        } else {
          pixels.splice(i, 1);
        }
      }
      
      // ============================================
      // 8. TORN LETTERS
      // ============================================
      tearLetters();
      
      for (let i = letters.length - 1; i >= 0; i--) {
        const L = letters[i];
        
        const dx = cx - L.x;
        const dy = cy - L.y;
        const dist = Math.hypot(dx, dy);
        
        const gravity = 1800 / (dist * dist + 70);
        
        L.orbitAngle += L.orbitSpeed * (1 + (1 - L.life) * 3);
        L.orbitRadius *= 0.97;
        
        const orbitX = Math.cos(L.orbitAngle) * L.orbitRadius * L.life;
        const orbitY = Math.sin(L.orbitAngle) * L.orbitRadius * L.life * 0.3;
        
        L.x += (dx / dist) * gravity + orbitX * 0.025;
        L.y += (dy / dist) * gravity + orbitY * 0.025;
        L.life -= L.decay;
        
        if (L.life > 0 && dist > 10) {
          const alpha = L.life * 0.92;
          const scale = L.life;
          const rotation = (1 - L.life) * 800;
          const stretch = 1 + (1 - L.life) * 2;
          
          ctx.save();
          ctx.translate(L.x, L.y);
          ctx.rotate(rotation * Math.PI / 180);
          ctx.scale(1, stretch);
          
          // Chromatic aberration
          if (L.life < 0.65) {
            const split = (0.65 - L.life) * 5;
            ctx.fillStyle = `rgba(255, 60, 80, ${alpha * 0.45})`;
            ctx.font = `${L.weight} ${L.size * scale}px Inter`;
            ctx.textAlign = 'center';
            ctx.textBaseline = 'middle';
            ctx.fillText(L.char, -split, 0);
            
            ctx.fillStyle = `rgba(60, 160, 255, ${alpha * 0.45})`;
            ctx.fillText(L.char, split, 0);
          }
          
          // Main letter
          if (L.isOutline) {
            ctx.strokeStyle = `rgba(232, 228, 220, ${alpha})`;
            ctx.lineWidth = 1;
            ctx.font = `${L.weight} ${L.size * scale}px Inter`;
            ctx.textAlign = 'center';
            ctx.textBaseline = 'middle';
            ctx.strokeText(L.char, 0, 0);
          } else {
            ctx.fillStyle = `rgba(232, 228, 220, ${alpha})`;
            ctx.font = `${L.weight} ${L.size * scale}px Inter`;
            ctx.textAlign = 'center';
            ctx.textBaseline = 'middle';
            ctx.fillText(L.char, 0, 0);
          }
          
          ctx.restore();
        } else {
          letters.splice(i, 1);
        }
      }
      
      requestAnimationFrame(render);
    }
    
    // ============================================
    // CLICK = MASSIVE BURST
    // ============================================
    document.addEventListener('click', () => {
      // Pixel explosion
      for (let i = 0; i < 80; i++) {
        if (pixels.length >= MAX_PIXELS) break;
        
        const angle = Math.random() * Math.PI * 2;
        const dist = 50 + Math.random() * 300;
        const px = mx + Math.cos(angle) * dist;
        const py = my + Math.sin(angle) * dist;
        
        if (px < 0 || px > W || py < 0 || py > H) continue;
        
        const color = samplePixelColor(px, py);
        
        pixels.push({
          x: px, y: py,
          size: 3 + Math.random() * 8,
          ...color,
          life: 1,
          decay: 0.012 + Math.random() * 0.012,
          orbitAngle: Math.random() * Math.PI * 2,
          orbitSpeed: 0.05 + Math.random() * 0.07,
          orbitRadius: 70 + Math.random() * 130,
          rotation: Math.random() * Math.PI * 2,
          rotationSpeed: (Math.random() - 0.5) * 0.5,
        });
      }
      
      // Letter explosion
      warpElements.forEach(el => {
        const rect = el.getBoundingClientRect();
        const text = el.textContent;
        if (!text) return;
        
        const dist = Math.hypot(mx - (rect.left + rect.width/2), my - (rect.top + rect.height/2));
        if (dist > 400) return;
        
        const style = getComputedStyle(el);
        const isOutline = el.classList.contains('outline');
        
        for (let i = 0; i < 6 && letters.length < MAX_LETTERS; i++) {
          const char = text[Math.floor(Math.random() * text.length)];
          if (!char.trim()) continue;
          
          letters.push({
            char,
            x: rect.left + Math.random() * rect.width,
            y: rect.top + Math.random() * rect.height,
            size: Math.min(parseFloat(style.fontSize) * 0.45, 45),
            weight: style.fontWeight,
            isOutline,
            life: 1,
            decay: 0.012 + Math.random() * 0.012,
            orbitAngle: Math.random() * Math.PI * 2,
            orbitSpeed: 0.045 + Math.random() * 0.065,
            orbitRadius: 80 + Math.random() * 120,
          });
        }
      });
      
      // Accretion burst
      for (let i = 0; i < 50; i++) {
        accretion.push({
          angle: Math.random() * Math.PI * 2,
          radius: 35 + Math.random() * 100,
          speed: 0.04 + Math.random() * 0.06,
          size: 2 + Math.random() * 3.5,
          life: 1,
          decay: 0.018 + Math.random() * 0.015,
          brightness: 0.6 + Math.random() * 0.4,
          type: Math.random() > 0.5 ? 'hot' : 'cold',
        });
      }
      
      // Tear lines burst
      for (let i = 0; i < 12; i++) {
        tearLines.push({
          x: cx,
          y: cy,
          angle: (Math.PI * 2 / 12) * i + Math.random() * 0.4,
          length: 80 + Math.random() * 120,
          life: 1,
          decay: 0.035,
        });
      }
      
      // Energy ring burst
      for (let i = 0; i < 3; i++) {
        energyRings.push({
          x: cx,
          y: cy,
          radius: 15 + i * 10,
          maxRadius: 200 + i * 50,
          life: 1,
          decay: 0.02 + i * 0.005,
        });
      }
    });
    
    // Recapture on scroll
    let scrollTimeout;
    window.addEventListener('scroll', () => {
      clearTimeout(scrollTimeout);
      scrollTimeout = setTimeout(captureSnapshot, 400);
    });
    
    // Start
    render();
    
    // Disable on touch
    if ('ontouchstart' in window) {
      canvas.style.display = 'none';
      samplerCanvas.style.display = 'none';
      chroma.style.display = 'none';
      document.querySelector('.scanlines').style.display = 'none';
    }
  </script>
</body>
</html>
