  <script src="https://html2canvas.hertzen.com/dist/html2canvas.min.js"></script>
  <script>
    // ================================================
    // PIXEL DECONSTRUCTION SINGULARITY
    // The website literally gets eaten by the cursor
    // ================================================
    
    const voidCanvas = document.getElementById('void');
    const voidCtx = voidCanvas.getContext('2d');
    const samplerCanvas = document.getElementById('sampler');
    const samplerCtx = samplerCanvas.getContext('2d', { willReadFrequently: true });
    
    let W, H;
    let mx = 0, my = 0;
    let cx = 0, cy = 0;
    let speed = 0;
    let time = 0;
    let isHovering = false;
    let pageSnapshot = null;
    let lastSnapshotTime = 0;
    
    // Pixel debris from the actual page
    const pixels = [];
    const MAX_PIXELS = 300;
    
    // Accretion disk particles
    const accretion = [];
    const MAX_ACCRETION = 100;
    
    // Resize
    function resize() {
      W = voidCanvas.width = window.innerWidth;
      H = voidCanvas.height = window.innerHeight;
      samplerCanvas.width = W;
      samplerCanvas.height = H;
      captureSnapshot();
    }
    resize();
    window.addEventListener('resize', resize);
    
    // Capture page snapshot for pixel sampling
    async function captureSnapshot() {
      const now = Date.now();
      if (now - lastSnapshotTime < 2000) return; // Throttle captures
      lastSnapshotTime = now;
      
      try {
        const content = document.getElementById('content');
        const canvas = await html2canvas(content, {
          backgroundColor: '#0a0a0a',
          scale: 0.5, // Lower res for performance
          logging: false,
          useCORS: true,
        });
        pageSnapshot = canvas;
        samplerCtx.drawImage(canvas, 0, 0, W, H);
      } catch(e) {
        // Fallback: create gradient snapshot
        createFallbackSnapshot();
      }
    }
    
    function createFallbackSnapshot() {
      // Sample colors from actual DOM elements
      samplerCtx.fillStyle = '#0a0a0a';
      samplerCtx.fillRect(0, 0, W, H);
      
      // Draw approximations of text areas
      const textEls = document.querySelectorAll('h1, h2, h3, p, a, span, .logo');
      textEls.forEach(el => {
        const rect = el.getBoundingClientRect();
        const style = getComputedStyle(el);
        const color = style.color || '#e8e4dc';
        samplerCtx.fillStyle = color;
        samplerCtx.globalAlpha = 0.8;
        samplerCtx.fillRect(rect.left, rect.top, rect.width, rect.height);
      });
      samplerCtx.globalAlpha = 1;
    }
    
    // Initial capture after page loads
    setTimeout(captureSnapshot, 500);
    
    // Mouse tracking
    document.addEventListener('mousemove', e => {
      speed = Math.hypot(e.clientX - mx, e.clientY - my);
      mx = e.clientX;
      my = e.clientY;
    });
    
    // Hover detection
    document.querySelectorAll('a, button, .project').forEach(el => {
      el.addEventListener('mouseenter', () => isHovering = true);
      el.addEventListener('mouseleave', () => isHovering = false);
    });
    
    // ================================================
    // SAMPLE ACTUAL PIXELS FROM PAGE
    // ================================================
    function samplePixelColor(x, y) {
      try {
        const data = samplerCtx.getImageData(
          Math.floor(x * 0.5), // Account for scale
          Math.floor(y * 0.5),
          1, 1
        ).data;
        return { r: data[0], g: data[1], b: data[2], a: data[3] };
      } catch(e) {
        return { r: 232, g: 228, b: 220, a: 255 };
      }
    }
    
    // ================================================
    // TEAR PIXELS FROM THE PAGE
    // ================================================
    function tearPixels() {
      if (pixels.length >= MAX_PIXELS) return;
      
      const tearRadius = 150;
      const tearIntensity = Math.min(speed / 10, 1) + 0.2;
      const numToSpawn = Math.floor(3 + speed * 0.5);
      
      for (let i = 0; i < numToSpawn; i++) {
        if (pixels.length >= MAX_PIXELS) break;
        if (Math.random() > tearIntensity) continue;
        
        // Random position around cursor
        const angle = Math.random() * Math.PI * 2;
        const dist = 20 + Math.random() * tearRadius;
        const px = mx + Math.cos(angle) * dist;
        const py = my + Math.sin(angle) * dist;
        
        // Skip if outside viewport
        if (px < 0 || px > W || py < 0 || py > H) continue;
        
        // Sample actual pixel color from page
        const color = samplePixelColor(px, py);
        
        // Skip very dark pixels (background)
        if (color.r < 20 && color.g < 20 && color.b < 20) {
          // Occasionally spawn dark pixels too for effect
          if (Math.random() > 0.1) continue;
        }
        
        // Pixel size varies
        const size = 2 + Math.random() * 6;
        
        pixels.push({
          x: px,
          y: py,
          originX: px,
          originY: py,
          size,
          color,
          life: 1,
          decay: 0.008 + Math.random() * 0.008,
          angle: Math.atan2(cy - py, cx - px),
          orbitAngle: Math.random() * Math.PI * 2,
          orbitSpeed: 0.03 + Math.random() * 0.05,
          orbitRadius: 30 + Math.random() * 60,
          rotation: Math.random() * Math.PI * 2,
          rotationSpeed: (Math.random() - 0.5) * 0.3,
        });
      }
    }
    
    // ================================================
    // ACCRETION DISK
    // ================================================
    function spawnAccretion() {
      if (accretion.length >= MAX_ACCRETION || Math.random() > 0.4) return;
      
      const angle = Math.random() * Math.PI * 2;
      const radius = 20 + Math.random() * 50;
      
      accretion.push({
        angle,
        radius,
        speed: 0.02 + Math.random() * 0.03,
        size: 1 + Math.random() * 2,
        life: 1,
        decay: 0.005 + Math.random() * 0.005,
        brightness: 0.3 + Math.random() * 0.7,
      });
    }
    
    // ================================================
    // MAIN RENDER LOOP
    // ================================================
    function render() {
      time += 0.016;
      
      // Smooth cursor
      cx += (mx - cx) * 0.12;
      cy += (my - cy) * 0.12;
      
      voidCtx.clearRect(0, 0, W, H);
      
      const intensity = Math.min(speed / 15, 1);
      const hoverScale = isHovering ? 1.2 : 1;
      
      // ============================================
      // 1. DESTRUCTION ZONE - Visual warping area
      // ============================================
      const zoneRadius = (180 + intensity * 60) * hoverScale;
      const zoneGrad = voidCtx.createRadialGradient(cx, cy, 0, cx, cy, zoneRadius);
      zoneGrad.addColorStop(0, `rgba(0, 0, 0, ${0.3 + intensity * 0.2})`);
      zoneGrad.addColorStop(0.3, `rgba(10, 10, 10, ${0.15 + intensity * 0.1})`);
      zoneGrad.addColorStop(0.7, `rgba(20, 20, 20, 0.05)`);
      zoneGrad.addColorStop(1, 'transparent');
      voidCtx.fillStyle = zoneGrad;
      voidCtx.beginPath();
      voidCtx.arc(cx, cy, zoneRadius, 0, Math.PI * 2);
      voidCtx.fill();
      
      // ============================================
      // 2. GRAVITATIONAL LENSING RINGS
      // ============================================
      for (let i = 0; i < 5; i++) {
        const ringRadius = (25 + i * 15) * hoverScale;
        const wobble = Math.sin(time * 2 + i * 0.5) * 2;
        const alpha = (0.08 - i * 0.012) * (1 + intensity * 0.5);
        
        voidCtx.strokeStyle = `rgba(232, 228, 220, ${alpha})`;
        voidCtx.lineWidth = 1;
        voidCtx.beginPath();
        voidCtx.arc(cx, cy, ringRadius + wobble, 0, Math.PI * 2);
        voidCtx.stroke();
      }
      
      // ============================================
      // 3. ACCRETION DISK
      // ============================================
      spawnAccretion();
      
      for (let i = accretion.length - 1; i >= 0; i--) {
        const p = accretion[i];
        
        p.angle += p.speed;
        p.radius *= 0.995;
        p.life -= p.decay;
        
        const ax = cx + Math.cos(p.angle) * p.radius * hoverScale;
        const ay = cy + Math.sin(p.angle) * p.radius * hoverScale * 0.35;
        
        const alpha = p.life * p.brightness;
        const glow = voidCtx.createRadialGradient(ax, ay, 0, ax, ay, p.size * 4);
        glow.addColorStop(0, `rgba(255, 240, 220, ${alpha})`);
        glow.addColorStop(0.5, `rgba(255, 200, 150, ${alpha * 0.5})`);
        glow.addColorStop(1, 'transparent');
        voidCtx.fillStyle = glow;
        voidCtx.beginPath();
        voidCtx.arc(ax, ay, p.size * 4, 0, Math.PI * 2);
        voidCtx.fill();
        
        if (p.life <= 0 || p.radius < 5) {
          accretion.splice(i, 1);
        }
      }
      
      // ============================================
      // 4. EVENT HORIZON
      // ============================================
      const horizonRadius = (14 + intensity * 6) * hoverScale;
      
      // Dark void
      const voidGrad = voidCtx.createRadialGradient(cx, cy, 0, cx, cy, horizonRadius * 2.5);
      voidGrad.addColorStop(0, 'rgba(0, 0, 0, 0.98)');
      voidGrad.addColorStop(0.4, 'rgba(0, 0, 0, 0.8)');
      voidGrad.addColorStop(0.7, 'rgba(0, 0, 0, 0.4)');
      voidGrad.addColorStop(1, 'transparent');
      voidCtx.fillStyle = voidGrad;
      voidCtx.beginPath();
      voidCtx.arc(cx, cy, horizonRadius * 2.5, 0, Math.PI * 2);
      voidCtx.fill();
      
      // Photon sphere ring
      voidCtx.strokeStyle = `rgba(255, 250, 240, ${0.5 + intensity * 0.3})`;
      voidCtx.lineWidth = 1.5;
      voidCtx.beginPath();
      voidCtx.arc(cx, cy, horizonRadius, 0, Math.PI * 2);
      voidCtx.stroke();
      
      // Core
      voidCtx.fillStyle = '#fff';
      voidCtx.shadowColor = '#fff';
      voidCtx.shadowBlur = 25 + intensity * 20;
      voidCtx.beginPath();
      voidCtx.arc(cx, cy, 3 * hoverScale, 0, Math.PI * 2);
      voidCtx.fill();
      voidCtx.shadowBlur = 0;
      
      // ============================================
      // 5. PIXEL DEBRIS - Actual page being consumed
      // ============================================
      tearPixels();
      
      for (let i = pixels.length - 1; i >= 0; i--) {
        const p = pixels[i];
        
        const dx = cx - p.x;
        const dy = cy - p.y;
        const dist = Math.hypot(dx, dy);
        
        // Strong gravitational pull
        const gravity = 1500 / (dist * dist + 80);
        
        // Orbital spiral
        p.orbitAngle += p.orbitSpeed * (1 + (1 - p.life) * 3);
        p.orbitRadius *= 0.98;
        
        const orbitX = Math.cos(p.orbitAngle) * p.orbitRadius * p.life;
        const orbitY = Math.sin(p.orbitAngle) * p.orbitRadius * p.life * 0.4;
        
        p.x += (dx / dist) * gravity + orbitX * 0.025;
        p.y += (dy / dist) * gravity + orbitY * 0.025;
        
        p.rotation += p.rotationSpeed;
        p.life -= p.decay;
        
        // Draw pixel
        if (p.life > 0 && dist > 8) {
          const alpha = p.life;
          const size = p.size * p.life;
          const { r, g, b } = p.color;
          
          voidCtx.save();
          voidCtx.translate(p.x, p.y);
          voidCtx.rotate(p.rotation);
          
          // Chromatic split for fast-moving pixels
          if (p.life < 0.6) {
            const split = (0.6 - p.life) * 4;
            voidCtx.fillStyle = `rgba(${Math.min(255, r + 50)}, ${g * 0.5}, ${b * 0.5}, ${alpha * 0.4})`;
            voidCtx.fillRect(-size/2 - split, -size/2, size, size);
            voidCtx.fillStyle = `rgba(${r * 0.5}, ${g * 0.5}, ${Math.min(255, b + 50)}, ${alpha * 0.4})`;
            voidCtx.fillRect(-size/2 + split, -size/2, size, size);
          }
          
          // Main pixel
          voidCtx.fillStyle = `rgba(${r}, ${g}, ${b}, ${alpha})`;
          voidCtx.fillRect(-size/2, -size/2, size, size);
          
          // Glow for bright pixels
          if (r > 150 || g > 150 || b > 150) {
            voidCtx.shadowColor = `rgb(${r}, ${g}, ${b})`;
            voidCtx.shadowBlur = 5 * alpha;
            voidCtx.fillRect(-size/2, -size/2, size, size);
            voidCtx.shadowBlur = 0;
          }
          
          voidCtx.restore();
        } else {
          pixels.splice(i, 1);
        }
      }
      
      requestAnimationFrame(render);
    }
    
    // Click burst
    document.addEventListener('click', () => {
      // Massive pixel burst
      for (let i = 0; i < 50; i++) {
        if (pixels.length >= MAX_PIXELS) break;
        
        const angle = Math.random() * Math.PI * 2;
        const dist = 30 + Math.random() * 200;
        const px = mx + Math.cos(angle) * dist;
        const py = my + Math.sin(angle) * dist;
        
        if (px < 0 || px > W || py < 0 || py > H) continue;
        
        const color = samplePixelColor(px, py);
        const size = 3 + Math.random() * 8;
        
        pixels.push({
          x: px,
          y: py,
          originX: px,
          originY: py,
          size,
          color,
          life: 1,
          decay: 0.01 + Math.random() * 0.01,
          angle: Math.atan2(cy - py, cx - px),
          orbitAngle: Math.random() * Math.PI * 2,
          orbitSpeed: 0.04 + Math.random() * 0.06,
          orbitRadius: 50 + Math.random() * 100,
          rotation: Math.random() * Math.PI * 2,
          rotationSpeed: (Math.random() - 0.5) * 0.4,
        });
      }
      
      // Accretion burst
      for (let i = 0; i < 30; i++) {
        accretion.push({
          angle: Math.random() * Math.PI * 2,
          radius: 25 + Math.random() * 70,
          speed: 0.03 + Math.random() * 0.05,
          size: 1.5 + Math.random() * 2.5,
          life: 1,
          decay: 0.01 + Math.random() * 0.01,
          brightness: 0.5 + Math.random() * 0.5,
        });
      }
    });
    
    // Recapture on scroll (throttled)
    let scrollTimeout;
    window.addEventListener('scroll', () => {
      clearTimeout(scrollTimeout);
      scrollTimeout = setTimeout(captureSnapshot, 300);
    });
    
    // Start
    render();
    
    // Disable on touch
    if ('ontouchstart' in window) {
      voidCanvas.style.display = 'none';
      samplerCanvas.style.display = 'none';
    }
  </script>
</body>
</html>
