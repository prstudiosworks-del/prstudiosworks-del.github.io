  <script src="https://html2canvas.hertzen.com/dist/html2canvas.min.js"></script>
  <script>
    // ================================================
    // CLEAN PIXEL DESTRUCTION - MOODBOARD EDITION
    // ================================================
    
    const canvas = document.getElementById('void');
    const ctx = canvas.getContext('2d');
    const samplerCanvas = document.getElementById('sampler');
    const samplerCtx = samplerCanvas.getContext('2d', { willReadFrequently: true });
    const chroma = document.querySelector('.chroma');
    const root = document.documentElement;
    const warpElements = document.querySelectorAll('.warp');
    
    let W, H;
    let mx = 0, my = 0;
    let cx = 0, cy = 0;
    let speed = 0;
    let lastMx = 0, lastMy = 0;
    let lastSnapshotTime = 0;
    
    const pixels = [];
    const MAX_PIXELS = 300;
    const WARP_RADIUS = 300;
    const TEAR_RADIUS = 200;
    
    function resize() {
      W = canvas.width = window.innerWidth;
      H = canvas.height = window.innerHeight;
      samplerCanvas.width = W;
      samplerCanvas.height = H;
      captureSnapshot();
    }
    resize();
    window.addEventListener('resize', resize);
    
    async function captureSnapshot() {
      const now = Date.now();
      if (now - lastSnapshotTime < 2500) return;
      lastSnapshotTime = now;
      
      try {
        const snapshot = await html2canvas(document.body, {
          backgroundColor: '#1a1715',
          scale: 0.5,
          logging: false,
          useCORS: true,
          ignoreElements: el => el.id === 'void' || el.id === 'sampler' || 
                               el.classList.contains('chroma') || 
                               el.classList.contains('scanlines') || 
                               el.classList.contains('grain')
        });
        samplerCtx.drawImage(snapshot, 0, 0, W, H);
      } catch(e) {
        samplerCtx.fillStyle = '#1a1715';
        samplerCtx.fillRect(0, 0, W, H);
        document.querySelectorAll('h1, h2, h3, p, a, span, img').forEach(el => {
          const rect = el.getBoundingClientRect();
          samplerCtx.fillStyle = getComputedStyle(el).color || '#f5f0e8';
          samplerCtx.fillRect(rect.left, rect.top, rect.width, rect.height);
        });
      }
    }
    
    setTimeout(captureSnapshot, 500);
    
    document.addEventListener('mousemove', e => {
      lastMx = mx;
      lastMy = my;
      mx = e.clientX;
      my = e.clientY;
      speed = Math.hypot(mx - lastMx, my - lastMy);
      
      root.style.setProperty('--mx', mx + 'px');
      root.style.setProperty('--my', my + 'px');
    });
    
    function samplePixelColor(x, y) {
      try {
        const data = samplerCtx.getImageData(
          Math.floor(x * 0.5),
          Math.floor(y * 0.5),
          1, 1
        ).data;
        return { r: data[0], g: data[1], b: data[2] };
      } catch(e) {
        return { r: 245, g: 240, b: 232 };
      }
    }
    
    function warpDOMElements() {
      let anyWarping = false;
      
      warpElements.forEach(el => {
        const rect = el.getBoundingClientRect();
        const elX = rect.left + rect.width / 2;
        const elY = rect.top + rect.height / 2;
        const dx = mx - elX;
        const dy = my - elY;
        const dist = Math.hypot(dx, dy);
        
        if (dist < WARP_RADIUS) {
          anyWarping = true;
          const influence = Math.pow(1 - dist / WARP_RADIUS, 2);
          const angle = Math.atan2(dy, dx);
          
          const pull = influence * 25;
          const pullX = Math.cos(angle) * pull;
          const pullY = Math.sin(angle) * pull;
          const rotation = influence * (dx > 0 ? 3 : -3);
          const skew = influence * 4 * Math.sign(dx);
          
          el.style.transform = `
            translate(${pullX}px, ${pullY}px)
            rotate(${rotation}deg)
            skewX(${skew}deg)
          `;
          
          if (dist < 150) {
            el.style.filter = `blur(${(1 - dist / 150) * 1.5}px)`;
          } else {
            el.style.filter = '';
          }
        } else {
          el.style.transform = '';
          el.style.filter = '';
        }
      });
      
      if (anyWarping || speed > 10) {
        chroma.classList.add('active');
      } else {
        chroma.classList.remove('active');
      }
    }
    
    function tearPixels() {
      if (pixels.length >= MAX_PIXELS) return;
      
      const tearIntensity = Math.min(speed / 10, 1) + 0.1;
      const numPixels = Math.floor(2 + speed * 0.3);
      
      for (let i = 0; i < numPixels; i++) {
        if (pixels.length >= MAX_PIXELS) break;
        if (Math.random() > tearIntensity) continue;
        
        const angle = Math.random() * Math.PI * 2;
        const dist = 20 + Math.random() * TEAR_RADIUS;
        const px = mx + Math.cos(angle) * dist;
        const py = my + Math.sin(angle) * dist;
        
        if (px < 0 || px > W || py < 0 || py > H) continue;
        
        const color = samplePixelColor(px, py);
        
        if (color.r < 30 && color.g < 28 && color.b < 26) {
          if (Math.random() > 0.1) continue;
        }
        
        pixels.push({
          x: px,
          y: py,
          size: 2 + Math.random() * 5,
          ...color,
          life: 1,
          decay: 0.008 + Math.random() * 0.008,
          vx: (Math.random() - 0.5) * 2,
          vy: (Math.random() - 0.5) * 2,
          rotation: Math.random() * Math.PI * 2,
          rotationSpeed: (Math.random() - 0.5) * 0.2,
        });
      }
    }
    
    function render() {
      cx += (mx - cx) * 0.1;
      cy += (my - cy) * 0.1;
      
      ctx.clearRect(0, 0, W, H);
      
      warpDOMElements();
      tearPixels();
      
      // Singularity void - warmer tones
      const voidRadius = 60 + Math.min(speed, 30);
      const voidGrad = ctx.createRadialGradient(cx, cy, 0, cx, cy, voidRadius);
      voidGrad.addColorStop(0, 'rgba(26, 23, 21, 0.95)');
      voidGrad.addColorStop(0.3, 'rgba(26, 23, 21, 0.6)');
      voidGrad.addColorStop(0.7, 'rgba(26, 23, 21, 0.2)');
      voidGrad.addColorStop(1, 'transparent');
      ctx.fillStyle = voidGrad;
      ctx.beginPath();
      ctx.arc(cx, cy, voidRadius, 0, Math.PI * 2);
      ctx.fill();
      
      // Core - warm ochre glow
      ctx.fillStyle = '#d4a45a';
      ctx.shadowColor = '#d4a45a';
      ctx.shadowBlur = 15 + speed * 0.5;
      ctx.beginPath();
      ctx.arc(cx, cy, 3, 0, Math.PI * 2);
      ctx.fill();
      ctx.shadowBlur = 0;
      
      // Pixels
      for (let i = pixels.length - 1; i >= 0; i--) {
        const p = pixels[i];
        
        const dx = cx - p.x;
        const dy = cy - p.y;
        const dist = Math.hypot(dx, dy);
        const gravity = 800 / (dist * dist + 50);
        
        p.vx += (dx / dist) * gravity;
        p.vy += (dy / dist) * gravity;
        p.vx *= 0.98;
        p.vy *= 0.98;
        
        p.x += p.vx;
        p.y += p.vy;
        p.rotation += p.rotationSpeed;
        p.life -= p.decay;
        
        if (p.life > 0 && dist > 8) {
          const size = p.size * p.life;
          
          ctx.save();
          ctx.translate(p.x, p.y);
          ctx.rotate(p.rotation);
          
          // Warm chromatic split
          if (p.life < 0.5) {
            const split = (0.5 - p.life) * 6;
            // Rust tint
            ctx.fillStyle = `rgba(${Math.min(255, p.r + 60)}, ${Math.max(0, p.g - 20)}, ${Math.max(0, p.b - 40)}, ${p.life * 0.5})`;
            ctx.fillRect(-size/2 - split, -size/2, size, size);
            // Dusty blue tint
            ctx.fillStyle = `rgba(${Math.max(0, p.r - 40)}, ${p.g}, ${Math.min(255, p.b + 60)}, ${p.life * 0.5})`;
            ctx.fillRect(-size/2 + split, -size/2, size, size);
          }
          
          ctx.fillStyle = `rgba(${p.r}, ${p.g}, ${p.b}, ${p.life})`;
          ctx.fillRect(-size/2, -size/2, size, size);
          
          ctx.restore();
        } else {
          pixels.splice(i, 1);
        }
      }
      
      requestAnimationFrame(render);
    }
    
    document.addEventListener('click', () => {
      for (let i = 0; i < 60; i++) {
        if (pixels.length >= MAX_PIXELS) break;
        
        const angle = Math.random() * Math.PI * 2;
        const dist = 30 + Math.random() * 250;
        const px = mx + Math.cos(angle) * dist;
        const py = my + Math.sin(angle) * dist;
        
        if (px < 0 || px > W || py < 0 || py > H) continue;
        
        const color = samplePixelColor(px, py);
        
        pixels.push({
          x: px,
          y: py,
          size: 3 + Math.random() * 6,
          ...color,
          life: 1,
          decay: 0.01 + Math.random() * 0.01,
          vx: (Math.random() - 0.5) * 4,
          vy: (Math.random() - 0.5) * 4,
          rotation: Math.random() * Math.PI * 2,
          rotationSpeed: (Math.random() - 0.5) * 0.3,
        });
      }
    });
    
    let scrollTimeout;
    window.addEventListener('scroll', () => {
      clearTimeout(scrollTimeout);
      scrollTimeout = setTimeout(captureSnapshot, 300);
    });
    
    render();
    
    if ('ontouchstart' in window) {
      canvas.style.display = 'none';
      samplerCanvas.style.display = 'none';
      chroma.style.display = 'none';
      document.querySelector('.scanlines').style.display = 'none';
    }
  </script>
</body>
</html>
